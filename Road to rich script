--Farm--

Tabs.Farm:AddButton({
    Title = "Farm Box Manuel",
    Description = "Do not delete the script if autofarm is enabled and you need TinyTask",
    Callback = function()
        Window:Dialog({
            Title = "Go Farm",
            Content = "✅",
            Buttons = {
                {
                    Title = "Start",
                    Callback = function()
                        isRunning = true
                        local player = game.Players.LocalPlayer
                        if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                            -- Positions de téléportation
                            local firstPosition = Vector3.new(938.8701782226562, 2562.83056640625, 152.9171142578125)
                            local secondPosition = Vector3.new(966.3018188476562, 2563.008056640625, 124.78646850585938)

                            while isRunning do
                                -- Téléportation au premier endroit
                                player.Character.HumanoidRootPart.CFrame = CFrame.new(firstPosition)
                                print("Téléporté au premier endroit. Attente de 4 secondes...")
                                task.wait(4) -- Attend 4 secondes

                                -- Téléportation au deuxième endroit
                                player.Character.HumanoidRootPart.CFrame = CFrame.new(secondPosition)
                                print("Téléporté au deuxième endroit. Attente de 4 secondes...")
                                task.wait(4) -- Attend 4 secondes
                            end
                        else
                            print("Erreur : le personnage ou le HumanoidRootPart n'a pas été trouvé.")
                        end
                    end
                },
                {
                    Title = "Stop",
                    Callback = function()
                        isRunning = false
                        print("Arrêt de la téléportation.")
                    end
                },
                {
                    Title = "Cancel",
                    Callback = function()
                        print("Cancelled the dialog.")
                    end
                }
            }
        })
    end
})

--1️⃣--

-- Ajout du bouton à l'interface utilisateur
Tabs.Farm:AddButton({
    Title = "Farm Seed Manuel",
    Description = "Do not delete the script if autofarm is enabled and you need TinyTask",
    Callback = function()
        Window:Dialog({
            Title = "Go Farm",
            Content = "✅",
            Buttons = {
                {
                    Title = "Start",
                    Callback = function()
                        isRunning = true  -- Démarre la boucle
                        local player = game.Players.LocalPlayer
                        
                        -- Vérifie que le personnage et le HumanoidRootPart existent
                        if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                            -- Positions de téléportation (7 positions)
                            local positions = {
                                Vector3.new(893.217041015625, 2563.0078125, 743.6980590820312),
                                Vector3.new(893.3887939453125, 2563.0078125, 745.8880004882812),
                                Vector3.new(893.3839721679688, 2563.0078125, 748.1597900390625),
                                Vector3.new(893.3584594726562, 2563.0078125, 750.181884765625),
                                Vector3.new(893.368408203125, 2563.0078125, 756.5469360351562),
                                Vector3.new(893.1061401367188, 2563.0078125, 758.7721557617188),
                                Vector3.new(893.2760620117188, 2563.0078125, 762.9422607421875)
                            }

                            while isRunning do
                                -- Boucle à travers toutes les positions
                                for _, pos in ipairs(positions) do
                                    player.Character.HumanoidRootPart.CFrame = CFrame.new(pos)  -- Téléporte le joueur
                                    print("Téléporté à la position : " .. tostring(pos) .. ". Attente de 4 secondes...")
                                    task.wait(4)  -- Attend 4 secondes
                                    if not isRunning then break end  -- Vérifie si l'on doit arrêter
                                end
                            end
                        else
                            print("Erreur : le personnage ou le HumanoidRootPart n'a pas été trouvé.")
                        end
                    end
                },
                {
                    Title = "Stop",
                    Callback = function()
                        isRunning = false  -- Met isRunning à false pour arrêter la téléportation
                        print("Arrêt de la téléportation.")
                    end
                },
                {
                    Title = "Cancel",
                    Callback = function()
                        print("Cancelled the dialog.")
                    end
                }
            }
        })
    end
})

Tabs.Farm:AddButton({
    Title = "Farm Steak Manuel",
    Description = "Do not delete the script if autofarm is enabled and you need TinyTask",
    Callback = function()
        Window:Dialog({
            Title = "Go Farm",
            Content = "✅",
            Buttons = {
                {
                    Title = "Start",
                    Callback = function()
                        isRunning = true
                        local player = game.Players.LocalPlayer
                        if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                            -- Positions de téléportation pour chaque étape
                            local positions = {
                                Vector3.new(462.3491516113281, 2563.3583984375, 1313.9193115234375),  -- Étape 1
                                Vector3.new(463.5831604003906, 2563.298828125, 1313.6759033203125),  -- Étape 2
                                Vector3.new(455.3656005859375, 2563.298828125, 1329.441162109375),  -- Étape 3
                                Vector3.new(456.14837646484375, 2563.298828125, 1313.6805419921875),  -- Étape 4
                                Vector3.new(456.14837646484375, 2563.298828125, 1313.6805419921875),  -- Étape 5
                                Vector3.new(448.9903564453125, 2563.298828125, 1329.5113525390625)   -- Étape 6
                            }

                            -- Temps d'attente pour chaque étape (en secondes)
                            local waitTime = 4  -- 4 secondes pour chaque étape

                            -- Fonction pour téléporter étape par étape
                            local function teleportSteps()
                                for stepIndex = 1, #positions do
                                    if isRunning then
                                        player.Character.HumanoidRootPart.CFrame = CFrame.new(positions[stepIndex])
                                        print("Téléporté à l'étape", stepIndex, ":", positions[stepIndex])
                                        task.wait(waitTime) -- Attendre 4 secondes
                                    end
                                end
                            end

                            -- Boucle infinie pour redémarrer les étapes
                            while isRunning do
                                teleportSteps() -- Exécute les téléportations
                            end
                        else
                            print("Erreur : le personnage ou le HumanoidRootPart n'a pas été trouvé.")
                        end
                    end
                },
                {
                    Title = "Stop",
                    Callback = function()
                        isRunning = false
                        print("Arrêt de la téléportation.")
                    end
                },
                {
                    Title = "Cancel",
                    Callback = function()
                        print("Cancelled the dialog.")
                    end
                }
            }
        })
    end
})

