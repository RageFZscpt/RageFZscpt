-- Création du dropdown dans le panneau principal
local Dropdown = Tabs.Tp:AddDropdown("Dropdown", {
    Title = "Select a Location",
    Values = {
        "none", 
        "library", 
        "phone store", 
        "steak restaurant", 
        "jewelry store", 
        "burger store", 
        "fitness store", 
        "laundry", 
        "alchimi", 
        "cars store", 
        "seed bunker", 
        "clothing store", 
        "usine", 
        "hair salon", 
        "dealer", 
        "métro", 
        "tattoo store"
    },
    Multi = false,
    Default = 1,  -- Définir "none" comme valeur par défaut
})

-- Définir la valeur initiale à "none"
Dropdown:SetValue("none")

-- Fonction appelée lors du changement de valeur dans le dropdown
Dropdown:OnChanged(function(Value)
    print("Dropdown changed:", Value)  -- Affiche la nouvelle valeur dans la console

    -- Récupérer le joueur local
    local player = game.Players.LocalPlayer

    -- Vérifiez que le personnage et le HumanoidRootPart existent
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        if Value == "none" then
            print("Aucune téléportation effectuée, reste au même endroit.")
        elseif Value == "library" then
            player.Character.HumanoidRootPart.CFrame = CFrame.new(50.89647674560547, 2562.90869140625, 1983.53955078125)
            print("Téléporté à la bibliothèque.")
